      and

      <\equation*>
        <around*|(|\<mu\><rsub|n>-\<mu\>|)><rsup|-><around*|(|X|)>\<equallim\><rsub|<text|<reference|Jordon
        Decomposition>>>sup<around*|(|<around*|{|-<around*|(|\<mu\><rsub|n>-\<mu\>|)><around*|(|A|)>\|A\<in\>\<cal-A\>\<wedge\>A\<subseteq\>X|}>|)>\<less\><frac|\<varepsilon\>|2>
