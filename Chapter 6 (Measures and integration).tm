    and

    <\equation*>
      \<lambda\><rsup|n>=<around*|(|\<varphi\><rsup|n>|)><rsub|\|\<cal-L\><around*|[|\<bbb-R\><rsup|n>|]>>:\<cal-L\><around*|[|\<bbb-R\><rsup|n>|]>\<rightarrow\><around*|[|0,\<infty\>|]>
