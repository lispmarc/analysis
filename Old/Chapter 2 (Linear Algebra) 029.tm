      <item><dueto|<math|j\<in\><around*|{|2,\<ldots\>,n|}>>>define then
      <math|M<rprime|'>> by <math|\<forall\>k,l\<in\><around*|{|1,\<ldots\>,n|}>>
      \ <math|M<rprime|'><rsub|k,l>=col<around*|(|M<rprime|'>,l|)><rsub|k>=col<around*|(|M,j\<rightsquigarrow\><rsub|n>1<around*|(|l|)>|)><rsub|k>=M<rsub|,j\<rightsquigarrow\><rsub|n>1<around*|(|l|)>>>
      then we have <math|\<forall\>k\<in\><around*|{|1,\<ldots\>,n|}>> that\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|row<around*|(|M<rprime|'>,1|)><rsub|k>>|<cell|=>|<cell|col<around*|(|M<rprime|'>,k|)><rsub|1>>>|<row|<cell|>|<cell|=>|<cell|col<around*|(|M,j\<rightsquigarrow\><rsub|n>1<around*|(|k|)>|)><rsub|1>>>|<row|<cell|>|<cell|=>|<cell|<choice|<tformat|<table|<row|<cell|col<around*|(|M,j|)><rsub|1>
        if k=1>>|<row|<cell|col<around*|(|M,k-1|)><rsub|1> if
        1\<less\>k\<leqslant\>j>>|<row|<cell|col<around*|(|M,k|)><rsub|1> if
