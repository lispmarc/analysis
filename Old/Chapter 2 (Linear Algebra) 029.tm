        <item><dueto|<math|i\<leqslant\>k\<leqslant\>n-1\<wedge\>l\<less\>j>>then\ 

        <\eqnarray*>
          <tformat|<table|<row|<cell|<around*|(|<around|[|1\<boxplus\>j|]>M<rprime|'>|)><rsub|k,l>>|<cell|\<equallim\><rsub|1\<leqslant\>k\<wedge\>l\<less\>j>>|<cell|M<rprime|'><rsub|k+1,l>>>|<row|<cell|>|<cell|=>|<cell|row<around*|(|M<rprime|'>,k+1|)><rsub|l>>>|<row|<cell|>|<cell|=>|<cell|row<around*|(|M,i\<rightsquigarrow\><rsub|n>1<around*|(|k+1|)>|)><rsub|l>>>|<row|<cell|>|<cell|\<equallim\><rsub|i\<less\>k+1><rsub|>>|<cell|row<around*|(|M,k+1|)><rsub|l>>>|<row|<cell|>|<cell|=>|<cell|M<rsub|k+1,l>>>|<row|<cell|>|<cell|\<equallim\><rsub|i\<leqslant\>k\<leqslant\>n-1\<wedge\>l\<less\>j>>|<cell|<around*|(|<around|[|i\<boxplus\>j|]>M|)><rsub|k,l>>>>>
