  <\proof>
    <math|\<forall\>i\<in\><around*|{|1,\<ldots\>,n|}>> we have

    <\eqnarray*>
      <tformat|<table|<row|<cell|<around*|(|L<rsub|2>\<circ\>L<rsub|1>|)><around*|(|e<rsub|i>|)>>|<cell|=>|<cell|L<rsub|2><around*|(|L<rsub|1><around*|(|e<rsub|i>|)>|)>>>|<row|<cell|>|<cell|=>|<cell|L<rsub|2><around*|(|<big|sum><rsub|k\<in\><around*|{|1,\<ldots\>,m|}>>\<cal-M\><around*|(|<around*|(|L<rsub|1>|\<nobracket\>>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)><rsub|k,i>\<cdot\>f<rsub|k>|)>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|k\<in\><around*|{|1,\<ldots\>,m|}>>\<cal-M\><around*|(|<around*|(|L<rsub|1>|\<nobracket\>>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)><rsub|k,i>\<cdot\>L<rsub|2><around*|(|f<rsub|k>|)>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|k\<in\><around*|{|1,\<ldots\>,m|}>>\<cal-M\><around*|(|L<rsub|1>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)><rsub|k,i>\<cdot\><around*|(|<big|sum><rsub|l\<in\><around*|{|1,\<ldots\>,r|}>>\<cal-M\><around*|(|L<rsub|2>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,m|}>>,<around*|{|g<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,r|}>>|)><rsub|l,k>\<cdot\>g<rsub|l>|)>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|l\<in\><around*|{|1,\<ldots\>,r|}>><around*|(|<big|sum><rsub|k\<in\><around*|{|1,\<ldots\>,m|}>>\<cal-M\><around*|(|L<rsub|1>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)><rsub|k,i>\<cdot\>\<cal-M\><around*|(|L<rsub|2>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,m|}>>,<around*|{|g<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,r|}>>|)><rsub|l,k>|)>\<cdot\>g<rsub|l>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|l\<in\><around*|{|1,\<ldots\>,r|}>><around*|(|<big|sum><rsub|k\<in\><around*|{|1,\<ldots\>,m|}>>\<cal-M\><around*|(|L<rsub|2>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,m|}>>,<around*|{|g<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,r|}>>|)><rsub|l,k>\<cdot\>\<cal-M\><around*|(|L<rsub|1>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)><rsub|k,i>|)>\<cdot\>g<rsub|l>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|l\<in\><around*|{|1,\<ldots\>,r|}>><around*|(|\<cal-M\><around*|(|L<rsub|2>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,m|}>>,<around*|{|g<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,r|}>>|)>\<cdot\>\<cal-M\><around*|(|L<rsub|1>,<around*|{|e<rsub|i>|}><rsub|i\<in\><around*|{|1,\<ldots\>,n|}>>,<around*|{|f<rsub|i>|}><rsub|i\<in\><around*|{|0,\<ldots\>,m|}>>|)>|)><rsub|l,i>\<cdot\>g<rsub|l>>>>>
