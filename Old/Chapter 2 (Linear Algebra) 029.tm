    <math|k\<in\>\<bbb-N\>\\<around*|{|1|}>> and
    <math|f:<big|prod><rsub|i=<around*|{|1,\<ldots\>,k|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>\<rightarrow\>A>
    then <math|<big|sum><rsub|\<gamma\>\<in\><big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k|}>>>f<around*|(|\<lambda\>|)>=<big|sum><rsub|\<gamma\>\<in\>\<Pi\><rsub|i\<in\><around*|{|1,\<ldots\>,k|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>>f<around*|(|\<gamma\><rsub|1>,\<ldots\>,y<rsub|k>|)>=<big|sum><rsub|\<gamma\>\<in\><big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>><around*|(|<big|sum><rsub|i\<in\><around*|{|m<rsub|k>,\<ldots\>,n<rsub|k>|}>>f<around*|(|\<gamma\><rsub|1>,\<ldots\>,\<gamma\><rsub|k-1>,i|)>|)><rsub|>>
  </theorem>

  <\proof>
    Define <math|<around*|{|x<rsub|<around*|(|i,j|)>>|}><rsub|<around*|(|i,j|)>\<in\><around*|(|<big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>|)>\<times\><around*|{|m<rsub|k>,\<ldots\>,n<rsub|k>|}>>>
    by <math|x<rsub|<around*|(|i,j|)>>=f<around*|(|i<rsub|1>,\<ldots\>,i<rsub|k-1>,j|)>>
    then using the previous theorem we have

    <\eqnarray*>
      <tformat|<table|<row|<cell|<big|sum><rsub|<around*|(|i,j|)>\<in\><around*|(|<big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>|)>\<times\><around*|{|m<rsub|k>,\<ldots\>,n<rsub|k>|}>>x<rsub|<around*|(|i,j|)>>>|<cell|=>|<cell|<big|sum><rsub|i\<in\><big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>><around*|(|<big|sum><rsub|j\<in\><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>>x<rsub|<around*|(|i,j|)>>|)>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|i\<in\><big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>><around*|(|<big|sum><rsub|j\<in\><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>>f<around*|(|i<rsub|1>,\<ldots\>,i<rsub|k-1>,j|)>|)>
      <around*|(|1|)>>>>>
    </eqnarray*>

    \ Using <reference|conventions for product of sets> (1) we have that
    <math|<big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>=<around*|(|<big|prod><rsub|i\<in\><around*|{|1,\<ldots\>,k-1|}>>|)>\<times\><around*|{|m<rsub|k>,\<ldots\>,n<rsub|k>|}>>
    so that\ 

    <\eqnarray*>
      <tformat|<table|<row|<cell|<big|sum><rsub|<around*|(|i,j|)>\<in\><around*|(|<big|prod><rsub|j\<in\><around*|{|1,\<ldots\>,k-1|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>|)>\<ast\><around*|{|m<rsub|k>,\<ldots\>,n<rsub|k>|}>>x<rsub|<around*|(|i,j|)>>>|<cell|=>|<cell|<big|sum><rsub|i\<in\><big|prod><rsub|j\<in\><around*|{|1,\<ldots\>,k|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|k>|}>>x<rsub|<around*|(|<around*|(|i<rsub|1>,\<ldots\>,i<rsub|k-1>|)>,i<rsub|k>|)>>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|i\<in\><big|prod><rsub|j\<in\><around*|{|1,\<ldots\>,k|}>><around*|{|m<rsub|i>,\<ldots\>,n<rsub|i>|}>>f<around*|(|i<rsub|1>,\<ldots\>,i<rsub|k-1>,i<rsub|k>|)>
      <around*|(|2|)>>>>>
