      and\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|col<around*|(|M<rprime|'>,j|)><rsub|k>>|<cell|=>|<cell|row<around*|(|M<rprime|'>,k|)><rsub|j>>>|<row|<cell|>|<cell|=>|<cell|row<around*|(|M,i\<rightsquigarrow\><rsub|n>1<around*|(|k|)>|)><rsub|j>>>|<row|<cell|>|<cell|=>|<cell|<choice|<tformat|<table|<row|<cell|row<around*|(|M,i|)><rsub|j>
        if k=1>>|<row|<cell|row<around*|(|M,k-1|)><rsub|j> if
        1\<less\>k\<leqslant\>i>>|<row|<cell|row<around*|(|M,k|)><rsub|j> if
        i\<less\>k>>>>>>>|<row|<cell|>|<cell|=>|<cell|<choice|<tformat|<table|<row|<cell|\<delta\><rsub|j,j>
        if k=1>>|<row|<cell|col<around*|(|M,j|)><rsub|k-1> if
        1\<less\>k\<leqslant\>i>>|<row|<cell|col<around*|(|M,j|)><rsub|k> if
        i\<less\>k>>>>>>>|<row|<cell|>|<cell|=>|<cell|<choice|<tformat|<table|<row|<cell|1
        if k=1>>|<row|<cell|\<delta\><rsub|i,k-1> if
