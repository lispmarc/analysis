          </eqnarray*>

          <item><dueto|<math|k\<neq\>1\<wedge\>k=j+1>>then\ 

          <\eqnarray*>
            <tformat|<table|<row|<cell|a<rsub|k>>|<cell|=>|<cell|<around*|(|t,x<rsub|1>,\<ldots\>,x<rsub|j-1>,x<rsub|j+1>,\<ldots\>,x<rsub|n>|)><rsub|i>>>|<row|<cell|>|<cell|\<equallim\><rsub|j+1\<less\>i>>|<cell|x<rsub|i>>>|<row|<cell|>|<cell|\<equallim\><rsub|x<rsub|i>=x<rsub|j>>>|<cell|x<rsub|j>>>|<row|<cell|>|<cell|\<equallim\><rsub|j+1\<less\>i>>|<cell|<around*|(|t,x<rsub|1>,\<ldots\>,x<rsub|i-1>,x<rsub|i+1>,\<ldots\>,x<rsub|n>|)><rsub|j+1>>>|<row|<cell|>|<cell|\<equallim\><rsub|k=j+1>>|<cell|<around*|(|t,x<rsub|1>,\<ldots\>,x<rsub|i-1>,x<rsub|i+1>,\<ldots\>,x<rsub|n>|)><rsub|k>>>>>
          </eqnarray*>

          <item><dueto|<math|k\<neq\>1\<wedge\>j+1\<less\>k\<leqslant\>i>>then\ 

          <\eqnarray*>
            <tformat|<table|<row|<cell|a<rsub|k>>|<cell|=>|<cell|<around*|(|t,x<rsub|1>,\<ldots\>,x<rsub|j-1>,x<rsub|j+1>,\<ldots\>,x<rsub|n>|)><rsub|k-1>>>|<row|<cell|>|<cell|\<equallim\><rsub|k\<leqslant\>i>>|<cell|<around*|(|t,x+1,\<ldots\>,x<rsub|i-1>,x<rsub|i+1>,\<ldots\>,x<rsub|n>|)><rsub|k>>>>>
