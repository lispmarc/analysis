        <\eqnarray*>
          <tformat|<table|<row|<cell|A<rsub|k>>|<cell|\<equallim\><rsub|k=1\<neq\>i+1=j>>|<cell|<around*|(|y,x<rsub|1>,\<ldots\>,x<rsub|i-1>,x<rsub|i+1>,\<ldots\>,x<rsub|n>|)><rsub|k>>>|<row|<cell|>|<cell|\<equallim\><rsub|k=1>>|<cell|y>>|<row|<cell|>|<cell|\<equallim\><rsub|k=1>>|<cell|<around*|(|y,x<rsub|1>,\<ldots\>,x<rsub|j-1>,x<rsub|j+1>,\<ldots\>,x<rsub|n>|)><rsub|k>>>>>
