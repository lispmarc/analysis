      and we have also\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|col<around*|(|M<rprime|'>,1|)><rsub|k>>|<cell|=>|<cell|col<around*|(|M,j\<rightsquigarrow\><rsub|n>1<around*|(|1|)>|)><rsub|k>>>|<row|<cell|>|<cell|=>|<cell|col<around*|(|M,j|)><rsub|k>>>|<row|<cell|>|<cell|=>|<cell|\<delta\><rsub|1,k>>>>>
