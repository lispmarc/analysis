      Now\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|<wide|\<Delta\>|\<breve\>><rsub|L><around*|(|x<rsub|1>,\<ldots\>,x<rsub|n>|)><around*|(|t|)>>|<cell|=>|<cell|<big|sum><rsub|j\<in\><around*|{|1,\<ldots\>,n|}>><around*|(|-1|)><rsup|j-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j-1>|)>,L<around*|(|x<rsub|j+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j>>>|<row|<cell|>|<cell|=>|<cell|<big|sum><rsub|j\<in\><around*|{|1,\<ldots\>,n|}>\\<around*|{|i<rsub|0>,j<rsub|0>|}>><around*|(|-1|)><rsup|j-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j-1>|)>,L<around*|(|x<rsub|j+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j>+<big|sum><rsub|j\<in\><around*|{|i<rsub|0>|}>><around*|(|-1|)><rsup|j-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j-1>|)>,L<around*|(|x<rsub|j+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j>+<big|sum><rsub|j\<in\><around*|{|j<rsub|0>|}>><around*|(|-1|)><rsup|j-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j-1>|)>,L<around*|(|x<rsub|j+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j>>>|<row|<cell|>|<cell|\<equallim\><rsub|<with|mode|text|<reference|eq
        10.73>>>>|<cell|<big|sum><rsub|j\<in\><around*|{|1,\<ldots\>,n|}>\\<around*|{|i<rsub|0>,j<rsub|0>|}>><around*|(|-1|)><rsup|j-1>\<cdot\>0\<cdot\>x<rsub|j>+<around*|(|-1|)><rsup|i<rsub|0>-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|i<rsub|0>-1>|)>,L<around*|(|x<rsub|i<rsub|0>+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|i<rsub|0>>+<around*|(|-1|)><rsup|j<rsub|0>-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j<rsub|0>-1>|)>,L<around*|(|x<rsub|j<rsub|0>+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j<rsub|0>>>>|<row|<cell|>|<cell|\<equallim\><rsub|<with|mode|text|<reference|eq
        10.74>>>>|<cell|<around*|(|-1|)><rsup|i<rsub|0>-1>\<cdot\><around*|(|-1|)><rsup|j<rsub|0>-i<rsub|0>-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j<rsub|0>-1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|i<rsub|0>>+<around*|(|-1|)><rsup|j<rsub|0>-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j<rsub|0>-1>|)>,L<around*|(|x<rsub|j<rsub|0>+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j<rsub|0>>>>|<row|<cell|>|<cell|\<equallim\><rsub|x<rsub|i<rsub|0>>=x<rsub|j<rsub|0>>>>|<cell|<around*|(|-1|)><rsup|j<rsub|0>-1>\<cdot\><around*|(|-1|)>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j<rsub|0>-1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j<rsub|0>>+<around*|(|-1|)><rsup|j<rsub|0>-1>\<cdot\>\<Delta\><around*|(|t,L<around*|(|x<rsub|1>|)>,\<ldots\>,L<around*|(|x<rsub|j<rsub|0>-1>|)>,L<around*|(|x<rsub|j<rsub|0>+1>|)>,\<ldots\>,L<around*|(|x<rsub|n>|)>|)>\<cdot\>x<rsub|j<rsub|0>>>>|<row|<cell|>|<cell|=>|<cell|0>>>>
