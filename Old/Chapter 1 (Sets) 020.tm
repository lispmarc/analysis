  <\proof>
    \ 

    <\eqnarray*>
      <tformat|<table|<row|<cell|x\<in\>\<um\>\<alpha\><rsub|r>>|<cell|\<Leftrightarrow\>>|<cell|-x\<nin\>\<alpha\><rsub|r>\<wedge\>-x\<neq\>min<around*|(|\<bbb-Q\>\<backslash\>\<alpha\><rsub|r>|)>>>|<row|<cell|>|<cell|\<Leftrightarrowlim\><rsub|<with|mode|text|<reference|rational
      cuts>>\<Rightarrow\>r=min<around*|(|\<bbb-Q\>\<backslash\>\<alpha\><rsub|r>|)>>>|<cell|-x\<nin\>\<alpha\><rsub|r>\<wedge\>-x\<neq\>r>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|r\<leqslant\>-x\<wedge\>-x\<neq\>r>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|-x\<gtr\>r>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<less\>\<um\>r>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<in\>\<alpha\><rsub|\<um\>r>>>>>
