  <\theorem>
    <label|generalized difference>If <math|A> is a class and
    <math|<around*|{|A<rsub|i>|)><rsub|i\<in\>I>> a family of classes then\ 

    <\enumerate>
      <item><math|A\\<around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>=<big|cup><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>

      <item><math|A\\<around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>=<big|cap><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>

      <item><math|<around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>\\A=<big|cup><rsub|i\<in\>I><around*|(|A<rsub|i>\\A|)>>

      <item><math|<around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>\\A=<big|cap><rsub|i\<in\>I><around*|(|A<rsub|i>\\A|)>>

      <item><math|A\<times\><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>=<big|cup><rsub|i\<in\>I><around*|(|A\<times\>A<rsub|i>|)>>

      <item><math|<around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>\<times\>A=<big|cup><rsub|i\<in\>I><around*|(|A<rsub|i>\<times\>A|)>>

      <item><math|<big|cap><rsub|i\<in\>I><around*|(|A\<times\>A<rsub|i>|)>=A\<times\><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>>

      <item><math|<big|cap><rsub|i\<in\>I><around*|(|A<rsub|i>\<times\>A|)>=<around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>\<times\>A>
    </enumerate>
  </theorem>

  <\proof>
    \ 

    <\enumerate>
      <item>We have\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|A\\<around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>>|<cell|=>|<cell|A<big|cap><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)><rsup|c>>>|<row|<cell|>|<cell|\<equallim\><rsub|<with|mode|text|<reference|generalized
        deMorgan's Laws>>>>|<cell|A<big|cap><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i><rsup|c>|)>>>|<row|<cell|>|<cell|\<equallim\><rsub|<with|mode|text|<reference|generalized
        distributive laws>>>>|<cell|<big|cup><rsub|i\<in\>I><around*|(|A<big|cap>A<rsup|c><rsub|i>|)>>>|<row|<cell|>|<cell|=>|<cell|<big|cup><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>>>>
      </eqnarray*>

      <item>We have

      <\eqnarray*>
        <tformat|<table|<row|<cell|A\\<around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>>|<cell|=>|<cell|A<big|cap><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)><rsup|c>>>|<row|<cell|>|<cell|=>|<cell|A<big|cap><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i><rsup|c>|)>>>>>
      </eqnarray*>

      Now if <math|x\<in\>A<big|cap><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i><rsup|c>|)>>
      then <math|x\<in\>A> and <math|\<forall\>i\<in\>I> we have
      <math|x\<nin\>A<rsub|i>\<Rightarrow\>x\<in\><big|cap><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>.
      If <math|x\<in\><big|cap><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>
      then <math|\<forall\>i\<in\>I> we have
      <math|x\<in\>A\<wedge\>x\<nin\>A<rsub|i>\<Rightarrow\>x\<in\>A\<wedge\>\<forall\>i\<in\>I\<vDash\>x\<in\>A<rsub|i><rsup|c>\<Rightarrow\>x\<in\>A<big|cap><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i><rsup|c>|)>>
      proving that <math|A<big|cap><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i><rsup|c>|)>=<big|cap><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>
      and thus that <math|A\\<around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>=<big|cap><rsub|i\<in\>I><around*|(|A\\A<rsub|i>|)>>.

      <item>We have <math|x\<in\><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>\\A\<Leftrightarrow\>x\<nin\>A\<wedge\>\<exists\>i\<in\>I\<vdash\>x\<in\>A<rsub|i>\<Leftrightarrow\>\<exists\>i\<in\>I\<vdash\><around*|(|x\<nin\>A\<wedge\>x\<in\>A<rsub|i>|)>\<Leftrightarrow\>x\<in\><big|cup><rsub|i\<in\>I><around*|(|A<rsub|i>\\A|)>>

      <item>We have <math|x\<in\><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>\\A\<Leftrightarrow\>x\<in\>A\<wedge\>\<forall\>i\<in\>I\<vdash\>x\<in\>A<rsub|i>\<Leftrightarrow\>\<forall\>i\<in\>I\<vDash\><around*|(|x\<in\>A<rsub|i>\<wedge\>x\<nin\>A|)>\<Leftrightarrow\>x\<in\><big|cap><rsub|i\<in\>I><around*|(|A<rsub|i>\\A|)>>

      <item>We have\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|<around*|(|x,y|)>\<in\>A\<times\><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>>|<cell|\<Leftrightarrow\>>|<cell|x\<in\>A\<wedge\>y\<in\><big|cup><rsub|i\<in\>I>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<in\>A\<wedge\>\<exists\>i\<in\>I\<vDash\>y\<in\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|\<exists\>i\<in\>I\<vDash\>x\<in\>A\<wedge\>y\<in\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|<around*|(|x,y|)>\<in\><big|cup><rsub|i\<in\>I><around*|(|A\<times\>A<rsub|i>|)>>>>>
      </eqnarray*>

      <item>We have

      <\eqnarray*>
        <tformat|<table|<row|<cell|<around*|(|x,y|)>\<in\><around*|(|<big|cup><rsub|i\<in\>I>A<rsub|i>|)>\<times\>A>|<cell|\<Leftrightarrow\>>|<cell|x\<in\><big|cup><rsub|i\<in\>I>A<rsub|i>\<wedge\>y\<in\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|y\<in\>A\<wedge\>\<exists\>i\<in\>I\<vDash\>x\<in\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|\<exists\>i\<in\>I\<vDash\>x\<in\>A<rsub|i>\<wedge\>y\<in\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|<around*|(|x,y|)>\<in\><big|cup><rsub|i\<in\>I><around*|(|A<rsub|i>\<times\>A|)>>>>>
      </eqnarray*>

      <item>We have\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|<around*|(|x,y|)>\<in\><big|cap><rsub|i\<in\>I><around*|(|A\<times\>A<rsub|i>|)>>|<cell|\<Leftrightarrow\>>|<cell|\<forall\>i\<in\>I\<vDash\><around*|(|x,y|)>\<in\>A\<times\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|\<forall\>i\<in\>I\<vDash\>x\<in\>A\<wedge\>y\<in\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<in\>A\<wedge\>\<forall\>i\<in\>I\<vDash\>y\<in\>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<in\>A\<wedge\>y\<in\><big|cap><rsub|i\<in\>I>A<rsub|i>>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|<around*|(|x,y|)>\<in\>A\<times\><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>>>>>
      </eqnarray*>

      <item>We have\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|<around*|(|x,y|)>\<in\><big|cap><rsub|i\<in\>I><around*|(|A<rsub|i>\<times\>A|)>>|<cell|\<Leftrightarrow\>>|<cell|\<forall\>i\<in\>I\<vDash\><around*|(|x,y|)>\<in\>A<rsub|i>\<times\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|\<forall\>i\<in\>I\<vDash\>x\<in\>A<rsub|i>\<wedge\>y\<in\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|<around*|(|\<forall\>i\<in\>I\<vDash\>x\<in\>A<rsub|i>|)>\<wedge\>y\<in\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|x\<in\><big|cap><rsub|i\<in\>I>A<rsub|i>\<wedge\>y\<in\>A>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|<around*|(|x,y|)>\<in\><around*|(|<big|cap><rsub|i\<in\>I>A<rsub|i>|)>\<times\>A>>>>
