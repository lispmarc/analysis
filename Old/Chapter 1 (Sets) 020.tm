  <\proof>
    \ \ 

    <\enumerate>
      <item><dueto|reflectivity>If <math|n\<in\>\<bbb-Z\>> then
      <math|n\<upl\><around*|(|\<um\>n|)>=0\<equallim\>\<sim\><around*|[|<around*|(|1,1|)>|]>=\<sim\><around*|[|<around*|(|s<around*|(|0|)>,1|)>|]>\<in\>\<bbb-N\><rsub|\<bbb-Z\>>>

      <item><dueto|anti--symmetry>If <math|n,m\<in\>\<bbb-Z\>>,
      <math|n\<leqslant\>m> and <math|m\<leqslant\>n> then
      <math|m\<upl\><around*|(|\<um\>n|)>\<in\>\<bbb-N\><rsub|\<bbb-Z\>>> and
      <math|n\<upl\><around*|(|\<um\>m|)>\<in\>\<bbb-N\><rsub|\<bbb-Z\>>>. As
      <math|n,m\<in\>\<bbb-Z\>> we have <math|n=\<sim\><around*|[|<around*|(|n<rsub|1>,n<rsub|2>|)>|]>>
      and <math|m=\<sim\><around*|[|<around*|(|m<rsub|1>,m<rsub|2>|)>|]>>
      then\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|n\<upl\><around*|(|\<um\>m|)>>|<cell|=>|<cell|\<sim\><around*|[|<around*|(|n<rsub|1>\<upl\>m<rsub|2>,n<rsub|2>\<upl\>m<rsub|1>|)>|]>=\<sim\><around*|[|<around*|(|s<around*|(|k|)>,1|)>|]>
        where k\<in\>\<bbb-N\>>>|<row|<cell|m\<upl\><around*|(|\<um\>n|)>>|<cell|=>|<cell|\<sim\><around*|[|<around*|(|m<rsub|1>\<upl\>n<rsub|2>,m<rsub|2>\<upl\>n<rsub|1>|)>|]>=\<sim\><around*|[|<around*|(|s<around*|(|l|)>,1|)>|]>
        where l\<in\>\<bbb-N\>>>>>
      </eqnarray*>

      we have then\ 

      <\eqnarray*>
        <tformat|<table|<row|<cell|n<rsub|1>\<upl\>m<rsub|2>\<upl\>1>|<cell|=>|<cell|n<rsub|2>\<upl\>m<rsub|1>\<upl\>s<around*|(|k|)>>>|<row|<cell|>|<cell|=>|<cell|n<rsub|2>\<upl\>m<rsub|1>\<upl\>k\<upl\>1>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|>>|<row|<cell|n<rsub|1>\<upl\>m<rsub|2>>|<cell|=>|<cell|n<rsub|2>\<upl\>m<rsub|1>\<upl\>k>>|<row|<cell|m<rsub|1>\<upl\>n<rsub|2>\<upl\>1>|<cell|=>|<cell|m<rsub|2>\<upl\>n<rsub|1>\<upl\>s<around*|(|l|)>>>|<row|<cell|>|<cell|=>|<cell|m<rsub|2>\<upl\>n<rsub|1>\<upl\>l\<upl\>1>>|<row|<cell|>|<cell|\<Leftrightarrow\>>|<cell|>>|<row|<cell|m<rsub|1>\<upl\>n<rsub|2>>|<cell|=>|<cell|m<rsub|2>\<upl\>n<rsub|1>\<upl\>l>>>>
      </eqnarray*>

