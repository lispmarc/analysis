  <\proof>
    If <math|z=<around*|(|x,y|)>,z<rprime|'>=<around*|(|x<rprime|'>,y<rprime|'>|)>>
    then\ 

    <\enumerate>
      <item><math|<wide|<around*|(|x,y|)>+<around*|(|x<rprime|'>,y<rprime|'>|)>|\<bar\>>=<wide|<around*|(|x+x<rprime|'>,y+y<rprime|'>|)>|\<bar\>>=<around*|(|x+x<rprime|'>,-y-y<rprime|'>|)>=<around*|(|x,-y|)>+<around*|(|x<rprime|'>,-y<rprime|'>|)>=<wide|<around*|(|x,y|)>|\<bar\>>+<wide|<around*|(|x<rprime|'>,y<rprime|'>|)>|\<bar\>>>

      <item><math|<wide|<around*|(|x,y|)>\<cdot\><around*|(|x<rprime|'>,y<rprime|'>|)>|\<bar\>>=<wide|<around*|(|x\<cdot\>x<rprime|'>-y\<cdot\>y<rprime|'>,x\<cdot\>y<rprime|'>+y\<cdot\>x<rprime|'>|)>|\<bar\>>=<around*|(|x\<cdot\>x<rprime|'>-y\<cdot\>y<rprime|'>,-x\<cdot\>y<rprime|'>-y\<cdot\>x<rprime|'>|)>=<around*|(|x\<cdot\>x<rprime|'>-<around*|(|-y|)>\<cdot\><around*|(|-y<rprime|'>|)>,x\<cdot\><around*|(|-y<rprime|'>|)>+<around*|(|-y|)>\<cdot\>x<rprime|'>|)>=*<around*|(|x,-y|)>\<cdot\><around*|(|x<rprime|'>,-y<rprime|'>|)>=<wide|<around*|(|x,y|)>|\<bar\>>\<cdot\><wide|<around*|(|x<rprime|'>,y<rprime|'>|)>|\<bar\>>>

      <item><math|<wide|<wide|z|\<bar\>>|\<bar\>>=<wide|<wide|<around*|(|x,y|)>|\<bar\>>|\<bar\>>=<wide|<around*|(|x,-y|)>|\<bar\>>=<around*|(|x,y|)>=z>
