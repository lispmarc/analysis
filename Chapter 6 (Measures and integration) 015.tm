      Next

      <\eqnarray*>
        <tformat|<table|<row|<cell|<around*|(|<wide|\<bbb-R\>|\<bar\>>\\A|)><big|cap>\<bbb-R\>>|<cell|=>|<cell|<around*|(|<wide|\<bbb-R\>|\<bar\>><big|cap>A<rsup|c>|)><big|cap>\<bbb-R\>>>|<row|<cell|>|<cell|=>|<cell|\<bbb-R\><big|cap>A<rsup|c>>>|<row|<cell|>|<cell|=>|<cell|\<bbb-R\>\\A>>|<row|<cell|>|<cell|\<in\>>|<cell|\<cal-B\><around*|[|\<bbb-R\>,\<cal-T\><rsub|<around*|\<\|\|\>||\<\|\|\>>>|]>
        <around*|(|see <reference|eq 16.44.484>|)>>>>>
